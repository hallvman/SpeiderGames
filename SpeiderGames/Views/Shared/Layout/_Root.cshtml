@{
    
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SpeiderGames</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/SpeiderGames.styles.css" asp-append-version="true" />
    <link rel="icon" href="/logo.png" sizes="32x32"/>
</head>
<body> 

@{
    await Html.RenderPartialAsync("/Views/Shared/Layout/Header.cshtml", false);
}

<div class="container" asp-area="" asp-controller="StartApp" asp-action="Index">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

@{
    await Html.RenderPartialAsync("/Views/Shared/Layout/Footer.cshtml", false);
}

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
